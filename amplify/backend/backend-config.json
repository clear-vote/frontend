{
  "storage": {
    "existingDynamoDBTable": {
      "service": "DynamoDB",
      "serviceType": "imported",
      "providerPlugin": "awscloudformation",
      "import": {
        "tableName": "${env.DYNAMO_TABLE_NAME}",
        "region": "${env.AWS_REGION}"
      }
    },
    "existingS3": {
      "service": "S3",
      "serviceType": "imported",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "import": {
        "bucketName": "${env.S3_BUCKET_NAME}"
      }
    }
  },
  "function": {
    "existingLambda": {
      "build": false,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "import": {
        "permissions": {
          "lambda": {
            "Arn": "${env.LAMBDA_ARN}"
          }
        }
      }
    }
  }
}